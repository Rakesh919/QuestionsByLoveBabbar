// Java program to reverse an array.. 

public class Demo
{

static void reverse(int arr[],int n){

 int s=0,e=n-1;
 while(s<e){
  int temp =arr[s];
  arr[s] =arr[e];
  arr[e] =temp;
  s++;
  e--;
 }
}
public static void main(String args[]){
  int arr[] ={1,2,3,4,5};
  
 reverse(arr,arr.length);
}
}
